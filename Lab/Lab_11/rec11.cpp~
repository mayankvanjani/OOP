#include <string>
#include <iostream>
using namespace std;

struct Node {
    Node(int data=0, Node* next=nullptr) : data(data), next(next) {}
    int data;
    Node* next;
};

void listInsertHead(Node*& headPtr, int data) {
    headPtr = new Node(data, headPtr);
}

void display( Node* hp ) {
  if ( !hp ) { cout << "Empty" << endl; }
  Node* p = hp;
  while ( p ) {
    cout << p->data << "  ";
    p = p->next;
  }
  cout << endl;
}

int main() {
  Node* ll = new Node(5);
  ll->next = new Node(7);
  ll->next->next = new Node(9);
  ll->next->next->next = new Node(1);
  display(ll);
}
